			<add-attribute value="/js/date.format.js" />
			<add-attribute value="/js/i18n.jquery/messages_vn.js" />
			<add-attribute value="/js/i18n.jquery.ui/ui.datepicker-en.js" />
			<add-attribute value="/js/i18n.jquery.ui/ui.datepicker-ja.js" />
			<add-attribute value="/js/i18n.jquery.ui/ui.datepicker-vn.js" />
			<add-attribute value="/js/jquery.metadata.js" />
			<add-attribute value="/js/vndirect.js" />
