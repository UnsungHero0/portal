				<reportElement key="staticText-34" x="11" y="80" width="41" height="12"/>
				<textElement textAlignment="Left">
					<font fontName="SansSerif" size="7" isItalic="false"/>
				</textElement>
				<text><![CDATA[1.2.]]></text>
			</staticText>
			<staticText>
				<reportElement key="staticText-35" x="11" y="231" width="41" height="14"/>
