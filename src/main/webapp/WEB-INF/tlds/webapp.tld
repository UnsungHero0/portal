 <!-- Define url tag -->
 <tag>
  <name>url</name>
  <tagclass>vn.com.vndirect.web.tag.URLTag</tagclass>
  <bodycontent>JSP</bodycontent>
  <attribute>
   <name>var</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>scope</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>value</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>rewrite</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>includeParam</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>removeParams</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
