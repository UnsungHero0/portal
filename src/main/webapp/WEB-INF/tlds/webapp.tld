  <attribute>
   <name>lang</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>orderBy</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <!-- String Utility Tags -->
 <tag>
  <name>strUtils</name>
  <tagclass>vn.com.vndirect.web.tag.StringUtilsTag</tagclass>
  <bodycontent>JSP</bodycontent>
  <info>String Utility: comapre, toLower/Upper case,...</info>
  <attribute>
   <name>var</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>scope</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>str</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>str1</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>str2</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>toLowerCase</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>compare</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>concat</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <!-- Define ConvertFileSizeTag tag -->
