 <!-- Define refData tag -->
 <tag>
  <name>refData</name>
  <tagclass>vn.com.vndirect.web.tag.RefDataTag</tagclass>
  <bodycontent>JSP</bodycontent>
  <info>Get List of ReferenceData and set to var in scope {default is request}</info>
  <attribute>
   <name>var</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>group</name>
   <required>true</required>
